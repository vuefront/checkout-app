{
    "name": "@vuefront/checkout-app",
    "version": "0.1.4",
    "description": "VueFront app",
    "files": [
        "lib",
        "assets",
        "src"
    ],
    "keywords": [
        "vuefront",
        "app",
        "vuefront-app"
    ],
    "main": "lib/index.js",
    "author": "VueFront",
    "license": "MIT",
    "scripts": {
        "build": "shx mkdir -p lib && shx rm -r lib && cross-env BUILD_DEV=1 NODE_ENV=esm babel src -D --watch --verbose --out-dir lib && echo {} > lib/.babelrc",
        "yalc-watch": "nodemon -e vue,js,scss,json -x yalc push"
    },
    "dependencies": {
        "@babel/standalone": "^7.12.12",
        "core-js": "^3.8.1",
        "lodash": "^4.17.20",
        "vue": "^2.6.12",
        "vuex": "^3.6.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.10",
        "@babel/core": "^7.12.10",
        "@babel/plugin-transform-modules-commonjs": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/polyfill": "^7.8.3",
        "@babel/preset-env": "^7.12.11",
        "babel-plugin-minify-dead-code-elimination": "^0.5.1",
        "babel-plugin-transform-inline-environment-variables": "^0.4.3",
        "cross-env": "^7.0.3",
        "nodemon": "^2.0.6",
        "shx": "^0.3.3"
    }
}
